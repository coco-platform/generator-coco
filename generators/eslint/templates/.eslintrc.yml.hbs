{{#if babel~}}
  parser: babel-eslint
{{/if}}
parserOptions:
  ecmaVersion: 2018
  sourceType: module
  ecmaFeatures:
    jsx: true
{{#if context.length ~}}
env:
{{/if}}
{{#each context}}
  {{this}}: true
{{/each}}
{{#if extends.length ~}}
extends:
{{/if}}
{{#each extends}}
  - '{{this}}'
{{/each}}
rules:
  linebreak-style:
    - error
    - unix
